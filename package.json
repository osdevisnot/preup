{
  "name": "preup",
  "version": "1.0.0",
  "bin": {
    "preup": "./src/preup.js"
  },
  "license": "MIT",
  "scripts": {
    "clean": "git clean -fdX",
    "postclean": "yarn --version || npm install -g yarn",
    "presetup": "npm run clean",
    "setup": "yarn",
    "postsetup": "npm link",
    "precommit": "lint-staged",
    "post-rewrite": "yarn",
    "post-merge": "yarn",
    "prepub": "npm publish",
    "pub": "docker build -t osdevisnot/preup .",
    "postpub": "docker push osdevisnot/preup"
  },
  "lint-staged": {
    "*.{js,css,json}": ["prettier --single-quote --no-semi --print-width 120 --write", "git add"],
    "*package.json": ["npm-sort", "git add"]
  },
  "dependencies": {
    "babel-core": "6.26.0",
    "babel-plugin-angularjs-annotate": "0.8.2",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babelrc-rollup": "3.0.0",
    "del": "3.0.0",
    "detective": "4.5.0",
    "less": "2.7.3",
    "node-sass": "4.6.0",
    "postcss-copy-assets": "0.3.0",
    "postcss-unprefix": "2.1.1",
    "rollup": "0.51.2",
    "rollup-load-plugins": "0.3.0",
    "rollup-plugin-babel": "3.0.2",
    "rollup-plugin-commonjs": "8.2.6",
    "rollup-plugin-filesize": "1.4.2",
    "rollup-plugin-html": "0.2.1",
    "rollup-plugin-image": "1.0.2",
    "rollup-plugin-json": "2.3.0",
    "rollup-plugin-license": "0.5.0",
    "rollup-plugin-postcss": "0.5.5",
    "rollup-plugin-replace": "2.0.0"
  },
  "devDependencies": {
    "husky": "0.14.3",
    "lint-staged": "4.3.0",
    "npm-sort": "0.0.4",
    "prettier": "1.8.2"
  }
}
