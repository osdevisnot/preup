{
  "name": "preup",
  "version": "2.0.10",
  "license": "MIT",
  "bin": {
    "preup": "./src/preup.js"
  },
  "scripts": {
    "clean": "git clean -fdX",
    "postclean": "yarn --version || npm install -g yarn",
    "prepub": "npm version patch",
    "pub": "npm publish",
    "postpub": "git push --follow-tags",
    "presetup": "npm run clean",
    "setup": "yarn",
    "postsetup": "npm link"
  },
  "dependencies": {
    "babel-core": "6.26.0",
    "babel-plugin-angularjs-annotate": "0.8.2",
    "babel-plugin-external-helpers": "6.22.0",
    "babel-plugin-styled-components": "1.5.1",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.6.1",
    "babel-preset-react": "6.24.1",
    "babelrc-rollup": "3.0.0",
    "del": "3.0.0",
    "detective": "5.1.0",
    "postcss-copy-assets": "0.3.1",
    "postcss-unprefix": "2.1.2",
    "postcss-value-parser": "3.3.0",
    "rollup": "0.58.1",
    "rollup-load-plugins": "0.3.0",
    "rollup-plugin-babel": "3.0.3",
    "rollup-plugin-cleanup": "2.0.1",
    "rollup-plugin-commonjs": "9.1.0",
    "rollup-plugin-filesize": "1.5.0",
    "rollup-plugin-html": "0.2.1",
    "rollup-plugin-image": "1.0.2",
    "rollup-plugin-json": "2.3.0",
    "rollup-plugin-license": "0.6.0",
    "rollup-plugin-postcss": "1.6.1",
    "rollup-plugin-replace": "2.0.0",
    "rollup-plugin-url": "1.4.0"
  },
  "devDependencies": {
    "husky": "0.15.0-rc.2",
    "lint-staged": "7.0.4",
    "prettier": "1.12.1",
    "sort-package-json": "1.14.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "post-rewrite": "yarn",
      "post-merge": "yarn"
    }
  },
  "lint-staged": {
    "*.{js,css}": ["prettier --write", "git add"],
    "*package.json": ["sort-package-json", "prettier --write", "git add"]
  }
}
